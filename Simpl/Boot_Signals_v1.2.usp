// STRING
/*******************************************************************************************
  Functions
  (Add any additional functions here)
  Note:  Functions must be physically placed before the location in
         the code that calls them.
*******************************************************************************************/
Function initFun()
{
	pulse(pulseLength, init);
}
Function initDelayFun()
{
	wait(delayTime);
	pulse(pulseLength, init_delayed);
}
Function initializingFun()
{
	pulse(initializingLength, initializing);
}
Function initializeAnalogs()
{
	integer x;
	For(x=1 to numAnalogSignals)
	{
    	Analog_Signal[x] = Analog_Value[x];
	}
}
/*******************************************************************************************
  Event Handlers
  (Uncomment and declare additional event handlers as needed)
*******************************************************************************************/
